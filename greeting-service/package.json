{
  "name": "nodejs-cache-redhat-greeting",
  "version": "3.0.0",
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "lint": "xo",
    "coverage": "nyc npm test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "ci": "npm run lint && npm run test",
    "start": "node .",
    "prepublish": "license-reporter report --silent",
    "openshift": "nodeshift --dockerImage=registry.access.redhat.com/ubi8/nodejs-12",
    "postinstall": "license-reporter report --silent && license-reporter save --xml licenses.xml --silent"
  },
  "main": "./bin/www",
  "xo": {
    "space": 2,
    "rules": {
      "space-before-function-paren": [
        "error",
        "always"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nodeshift-starters/nodejs-cache.git"
  },
  "files": [
    "package.json",
    "LICENSE",
    "bin",
    "lib",
    "public",
    "app.js",
    "licenses"
  ],
  "bugs": {
    "url": "https://github.com/nodeshift-starters/nodejs-cache/issues"
  },
  "homepage": "https://github.com/nodeshift-starters/nodejs-cache",
  "devDependencies": {
    "nodeshift": "~4.1.0",
    "nyc": "~14.1.1",
    "proxyquire": "~2.1.3",
    "supertest": "~4.0.2",
    "tap-spec": "~5.0.0",
    "tape": "~4.12.0",
    "xo": "~0.25.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "~1.19.0",
    "express": "~4.17.1",
    "infinispan": "~0.6.0",
    "kube-probe": "~0.3.2",
    "license-reporter": "~1.2.2"
  }
}
